{
  "name": "raise-the-bar-chart-presentation",
  "version": "0.0.1",
  "description": "Raise the Bar (Chart)",
  "scripts": {
    "clean": "rimraf dist",
    "build": "webpack",
    "start": "webpack-dev-server --hot",
    "pdf": "decktape generic --size=1920x1080 --max-slides=98 http://localhost:8080/ slides.pdf",
    "pdf2": "decktape generic --size=1920x1080 --max-slides=98 --screenshots --screenshots-size=1920x1080 dist/index.html slides.pdf",
    "export": "npm run build && npm run pdf2",
    "merge": "convert screenshots/slides_* slides.pdf"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "spectacle": "^7.0.1",
    "victory": "^35.4.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.8",
    "@babel/preset-react": "^7.12.7",
    "@mdx-js/react": "^1.6.22",
    "babel-loader": "^8.0.6",
    "decktape": "^3.1.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "raw-loader": "^4.0.0",
    "rimraf": "^3.0.0",
    "spectacle-mdx-loader": "^0.1.1",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}